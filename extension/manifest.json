{
    "manifest_version": 2,
    "name": "Swarm",
    "description": "Enroll as an anonymous egress point in the hive",
    "version": "0.0.1",
    "icons": {
        "16": "resources/ico/16.png",
        "48": "resources/ico/48.png",
        "128": "resources/ico/128.png"
    },
    "browser_action": {
        "default_title": "Swarm",
        "default_icon": "resources/ico/16.png"
    },
    "options_page": "resources/static/options.html",
    "permissions": ["http://*/", "https://*/", "alarms", "notifications"],
    "background": {
        "scripts": ["resources/external/jquery.js",
                    "resources/external/socket.io.min.js",
                    "resources/js/core/background.js"],
        "persistent": true
    }
}
